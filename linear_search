#include <stdio.h>
int main()
{
	int a[10],i,key,n,found=0;
	printf("enter no of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
	printf("enter array:");
	scanf("%d",&a[i]);
}
	printf("enter element to search:");
	scanf("%d",&key);
	for(i=0;i<n;i++)
	{
		if(a[i]==key){
		printf("element is found in index %d\n",i);
		break;
	}
}
	if(i==n)
	printf("element is not found");
	}
